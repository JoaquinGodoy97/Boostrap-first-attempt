{
  "name": "first-repository",
  "version": "1.0.0",
  "description": "Primer projecto de Node Js",
  "main": "index.html",
  "scripts": {
    "dev": "lite-server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "concurrently \"npm run watch:scss\" \"npm run dev\"",
    "scss": "node-sass -o F:/Workplace/Curso/Boostrap/Repositorio/first-repository/css/ F:/Workplace/Curso/Boostrap/Repositorio/first-repository/css",
    "watch:scss": "onchange \"css/*.scss\" -- npm run scss",
    "imagemin": "imagemin \"./css/images/*\" --out-dir \"./dist/images\"",
    "clean": "rimraf ./dist",
    "usemin": "usemin index.html -d dist --htmlmin -o dist/index.html && usemin contacto.html -d dist --htmlmin -o dist/contacto.html && usemin about.html -d dist --htmlmin -o dist/about.html && usemin reciclaje.html -d dist --htmlmin -o dist/reciclaje.html",
    "build": "npm run clean && npm run scss & npm run imagemin && npm run usemin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://JoaquinGodoy@bitbucket.org/JoaquinGodoy/first-repository.git"
  },
  "author": "Joaquin Godoy",
  "license": "ISC",
  "homepage": "https://bitbucket.org/JoaquinGodoy/first-repository#readme",
  "dependencies": {
    "@popperjs/core": "^2.5.3",
    "bootstrap": "^4.5.2",
    "concurrently": "^5.3.0",
    "imagemin-cli": "^6.0.0",
    "jquery": "^3.5.1",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "copyfiles": "^2.4.0",
    "cssmin": "^0.4.3",
    "htmlmin": "0.0.7",
    "lite-server": "^2.5.4",
    "node-sass": "^4.14.1",
    "onchange": "^7.0.2",
    "rimraf": "^3.0.2",
    "uglify-js": "^3.11.2",
    "usemin-cli": "^0.6.0"
  }
}
