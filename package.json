{
  "name": "first-repository",
  "version": "1.0.0",
  "description": "Primer projecto de Node Js",
  "main": "index.html",
  "scripts": {
    "dev": "lite-server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "concurrently \"npm run watch:scss\" \"npm run dev\"",
    "scss": "node-sass -o ./css ./css",
    "watch:scss": "onchange 'css/style.scss' -- npm run scss",
    "clean": "rimraf dist",
    "imagemin": "imagemin css/images/* --out-dist dist/css/images"
  },
  "repository": {
    "type": "git",
    "url": "git+https://JoaquinGodoy@bitbucket.org/JoaquinGodoy/first-repository.git"
  },
  "author": "Joaquin Godoy",
  "license": "ISC",
  "homepage": "https://bitbucket.org/JoaquinGodoy/first-repository#readme",
  "dependencies": {
    "@popperjs/core": "^2.5.3",
    "bootstrap": "^4.5.2",
    "concurrently": "^5.3.0",
    "imagemin-cli": "^6.0.0",
    "jquery": "^3.5.1",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "lite-server": "^2.5.4",
    "node-sass": "^4.14.1",
    "onchange": "^7.0.2",
    "rimraf": "^3.0.2"
  }
}
